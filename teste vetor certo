
    public static void main(String[] args){
      
        int[] m = {2,1,5,3,4};
        
        int x = 0;
        
        for(int i=0; i<=4;i++){
            for(int j=0; j<=4; j++){
                if(m[i]<m[j]){
                  x = m[i];
                  m[i] = m[j];
                  m[j] = x;
                }
            }
        }
        
        for(int i=0;i<=4;i++){
            System.out.println(m[i]);
        }
        
        
    } 
}
